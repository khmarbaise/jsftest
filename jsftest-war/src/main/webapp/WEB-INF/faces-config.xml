<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

	<navigation-rule>
		<from-view-id>/login.xthml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/success.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/error.xthml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/success.xthml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/version.xthml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/success.xthml</to-view-id>
		</navigation-case>
	</navigation-rule>

<!-- 	<application> -->
<!-- 		<message-bundle>com.sun.faces.resources.Messages</message-bundle> -->
<!-- 	</application> -->
	
</faces-config>
